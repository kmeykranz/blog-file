formulas:
  date: date
views:
  - type: table
    name: Table
    filters:
      and:
        - file.inFolder("content")
    groupBy:
      property: file.folder
      direction: ASC
    order:
      - file.name
      - formula.date
    sort:
      - property: formula.date
        direction: DESC
      - property: file.name
        direction: ASC
    columnSize:
      file.name: 503
